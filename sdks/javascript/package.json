{
  "name": "@hivehub/vectorizer-sdk-js",
  "version": "2.1.0",
  "type": "module",
  "description": "JavaScript SDK for Vectorizer - High-performance vector database",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "node examples/basic-usage.js",
    "publish:wsl": "export BROWSER=wslview && npm publish"
  },
  "keywords": [
    "vector-database",
    "semantic-search",
    "embeddings",
    "hnsw",
    "similarity-search",
    "javascript",
    "hivehub"
  ],
  "author": "HiveLLM Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hivellm/vectorizer.git",
    "directory": "client-sdks/javascript"
  },
  "bugs": {
    "url": "https://github.com/hivellm/vectorizer/issues"
  },
  "homepage": "https://github.com/hivellm/vectorizer#readme",
  "optionalDependencies": {
    "@hivellm/umicp": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.0",
    "rollup": "^4.18.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
