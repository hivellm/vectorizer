{
  "name": "@hivehub/vectorizer-sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for Vectorizer - High-performance vector database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "dev": "ts-node examples/basic-usage.ts",
    "publish:wsl": "export BROWSER=wslview && npm publish"
  },
  "keywords": [
    "vector-database",
    "semantic-search",
    "embeddings",
    "hnsw",
    "similarity-search",
    "typescript",
    "hivehub"
  ],
  "author": "HiveLLM Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hivellm/vectorizer.git",
    "directory": "sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/hivellm/vectorizer/issues"
  },
  "homepage": "https://github.com/hivellm/vectorizer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.0",
    "js-yaml": ">=4.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
