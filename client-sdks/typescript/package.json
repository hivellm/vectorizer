{
  "name": "@hivellm/vectorizer-client",
  "version": "1.0.0",
  "description": "TypeScript client for Hive Vectorizer - High-performance vector database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "ts-node examples/basic-usage.ts",
    "publish:wsl": "export BROWSER=wslview && npm publish"
  },
  "keywords": [
    "vector-database",
    "semantic-search",
    "embeddings",
    "hnsw",
    "similarity-search",
    "typescript",
    "hivellm"
  ],
  "author": "HiveLLM Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hivellm/vectorizer.git",
    "directory": "client-sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/hivellm/vectorizer/issues"
  },
  "homepage": "https://github.com/hivellm/vectorizer#readme",
  "dependencies": {
    "@hivellm/umicp": "^0.1.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
