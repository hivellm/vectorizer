{
  "name": "@hivellm/vectorizer-client-js",
  "version": "0.4.1",
  "description": "JavaScript client for Hive Vectorizer - High-performance vector database",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "node examples/basic-usage.js",
    "publish:wsl": "export BROWSER=wslview && npm publish"
  },
  "keywords": [
    "vector-database",
    "semantic-search",
    "embeddings",
    "hnsw",
    "similarity-search",
    "javascript",
    "hivellm"
  ],
  "author": "HiveLLM Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hivellm/vectorizer.git",
    "directory": "client-sdks/javascript"
  },
  "bugs": {
    "url": "https://github.com/hivellm/vectorizer/issues"
  },
  "homepage": "https://github.com/hivellm/vectorizer#readme",
  "dependencies": {
    "@hivellm/umicp": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.18.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
